<?xml version="1.0" encoding="utf-8" ?>
<pages:MauiPage x:Class="maui_lotto.Views.ResultPage"
                xmlns="http://schemas.microsoft.com/dotnet/2021/maui"                             
                xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                xmlns:pages="clr-namespace:maui_lotto.Views"
                xmlns:local="clr-namespace:maui_lotto.Views"
                xmlns:vm="clr-namespace:maui_lotto.ViewModels"
                Title="{Binding Title}"
                x:DataType="vm:ResultViewModel">
    <ContentPage.BindingContext>
        <vm:ResultViewModel />
    </ContentPage.BindingContext>

    <ContentPage.Content>
        <Grid Style="{StaticResource ContentArea}">
            <Button Text="테스트" Command="{Binding TestCommand}"/>
        </Grid>
    </ContentPage.Content>    
</pages:MauiPage>